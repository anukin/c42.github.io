---
portfolio_selected_class: selected
title: C42 Engineering | portfolio
---

<div id="portfolio">
  <% data.projects.each do |project| %>
    <div class="project">
      <div class="name highlight">
	<%= project.name %>
      </div>
      <div class="keywords highlight">
	Keywords:
	<ul>
	  <% project.keywords.each do |kw| %>
	    <li><%= kw %></li>
	  <% end %>
	</ul>
      </div>
      <div class="locations highlight">
	Locations:
	<ul>
	  <% project.locations.each do |location| %>
	    <li><%= location %></li>
	  <% end %>
	</ul>
      </div>
      <div class="description">
	<div class="highlight">Project Description</div>
	<% project.description.each do |desc| %>
	  <p><%= desc %></p>
	<% end %>
      </div>
    </div>
  <% end %>
</div>
